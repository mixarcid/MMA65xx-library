#include <SPI.h>

enum CommandMsg {
  AX_X = 0 << 14,
  AX_Y = 1 << 14,
  A_REG_OP = 0 << 13,
  A_DATA_REQ = 1 << 13,
  OC_OFFSET_CANCELLED = 0 << 12,
  OC_RAW_ACCEL = 1 << 12,
  SD_SIGNED_DATA = 0 << 2,
  SD_UNSIGNED_DATA = 1 << 2,
  ARM_PCM = 0 << 1,
  ARM_ARMING = 1 << 1,
  P_ODD = 0 << 0,
  P_EVEN = 1 << 0
};

enum ResponseMsg {

};
